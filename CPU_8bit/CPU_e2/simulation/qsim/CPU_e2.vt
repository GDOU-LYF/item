module CPU_e2_vlg_vec_tst();
// constants                                           
// general purpose registers
reg clk;
reg [3:0] JMP;
reg load_data;
reg load_JMP;
reg [1:0] S;
reg show_R;
reg start;
// wires                                               
wire [7:0] A;
wire [7:0] B;
wire cout;
wire [7:0] F;
wire [6:0] HEX0;
wire [6:0] HEX1;
wire [6:0] HEX2;
wire [6:0] HEX3;
wire [6:0] HEX4;
wire [6:0] HEX5;
wire [6:0] HEX6;
wire [6:0] HEX7;
wire [7:0] LEDR;
wire overflow;
wire [7:0] R0;
wire [7:0] R1;
wire [7:0] R2;
wire [7:0] R3;
wire show_clk;
wire show_start;

wire sampler;                             

// assign statements (if any)                          
CPU_e2 i1 (
// port map - connection between master ports and signals/registers   
	.A(A),
	.B(B),
	.clk(clk),
	.cout(cout),
	.F(F),
	.HEX0(HEX0),
	.HEX1(HEX1),
	.HEX2(HEX2),
	.HEX3(HEX3),
	.HEX4(HEX4),
	.HEX5(HEX5),
	.HEX6(HEX6),
	.HEX7(HEX7),
	.JMP(JMP),
	.LEDR(LEDR),
	.load_data(load_data),
	.load_JMP(load_JMP),
	.overflow(overflow),
	.R0(R0),
	.R1(R1),
	.R2(R2),
	.R3(R3),
	.S(S),
	.show_clk(show_clk),
	.show_R(show_R),
	.show_start(show_start),
	.start(start)
);

// show_R
initial
begin
	show_R = 1'b0;
end 

// clk
always
begin
	clk = 1'b0;
	clk = #5000 1'b1;
	#5000;
end 

// start
initial
begin
	start = 1'b0;
	start = #50000 1'b1;
end 
